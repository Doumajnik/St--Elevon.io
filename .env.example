# Entry URL for job listings
START_URL=https://www.profesia.sk/

# Maximum number of parallel threads for scraping
CONCURRENCY=4

# Maximum number of pages to scrape
MAX_PAGES=10

# Output directory for scraped job listings
OUTPUT_DIR=./output

# Maximum requests per minute
MAX_REQUESTS_PER_MINUTE=100

# Maximum request retries
MAX_REQUEST_RETRIES=3

# Request handler timeout in seconds
REQUEST_HANDLER_TIMEOUT_SECS=60

# Enable detailed logging (true/false)
VERBOSE=false